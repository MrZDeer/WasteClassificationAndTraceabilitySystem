
 <view class="table">
 <view class="trs bg-header">
  <view class="th">住户姓名</view>
  <view class="th">身份证号</view>
  <view class="th">地址</view>
  
  <view class="th">抽检时间</view>
 </view>
 <block wx:for="{{listData}}" wx:for-item='item' >
  <view class="tr bg-items">
        <view class="td">{{item.name}}</view>
        <view class="td">{{item.idcard}}</view>
        <view class="td">{{item.address}}</view>
       
        <view class="td">{{item.time}}</view>
  </view>
 </block>
</view>
